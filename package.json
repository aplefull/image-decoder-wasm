{
  "name": "image-decoder-wasm",
  "version": "0.1.0",
  "description": "Browser image decoder library using WebAssembly for unsupported formats",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "wasm",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "build:wasm": "./scripts/build-all.sh",
    "build:wasm:avif": "./scripts/build-avif.sh",
    "build:wasm:heif": "./scripts/build-heif.sh",
    "build:wasm:webp": "./scripts/build-webp.sh",
    "build:wasm:jxl": "./scripts/build-jxl.sh",
    "build:wasm:jxr": "./scripts/build-jxr.sh",
    "build:wasm:jpeg": "./scripts/build-jpeg.sh",
    "build:wasm:jpegls": "./scripts/build-jpegls.sh",
    "build:wasm:tiff": "./scripts/build-tiff.sh",
    "build:wasm:raw": "./scripts/build-raw.sh",
    "dev": "vite",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --project=unit",
    "test:unit:ui": "vitest --project=unit --ui",
    "test:browser": "vitest run --project=browser",
    "test:browser:ui": "vitest --project=browser --ui",
    "docs:dev": "cd docs && pnpm dev",
    "docs:build": "cd docs && pnpm build",
    "docs:start": "cd docs && pnpm start"
  },
  "keywords": [
    "image",
    "decoder",
    "wasm",
    "webassembly",
    "avif",
    "heif",
    "heic",
    "webp",
    "jxl",
    "jpeg",
    "jpegls",
    "jpeg-ls",
    "tiff",
    "raw",
    "cr2",
    "nef",
    "arw",
    "dng"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aplefull/image-decoder-wasm.git"
  },
  "bugs": {
    "url": "https://github.com/aplefull/image-decoder-wasm/issues"
  },
  "homepage": "https://github.com/aplefull/image-decoder-wasm#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/pixelmatch": "^5.2.6",
    "@vitest/browser": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.56.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.2.4"
  }
}
