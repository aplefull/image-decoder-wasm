{
  "name": "image-decoder-wasm",
  "version": "0.1.0",
  "description": "Browser image decoder library using WebAssembly for unsupported formats",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:wasm": "./scripts/build-all.sh",
    "build:wasm:avif": "./scripts/build-avif.sh",
    "build:wasm:heif": "./scripts/build-heif.sh",
    "build:wasm:webp": "./scripts/build-webp.sh",
    "build:wasm:jxl": "./scripts/build-jxl.sh",
    "build:wasm:jpeg": "./scripts/build-jpeg.sh",
    "copy-wasm": "./scripts/copy-wasm.sh",
    "dev": "vite",
    "test": "node test/decoder.test.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "image",
    "decoder",
    "wasm",
    "webassembly",
    "avif",
    "heif",
    "heic",
    "webp",
    "jxl",
    "jpeg"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-wasm": "^6.2.2",
    "@types/node": "^20.11.0",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^7.1.9"
  }
}
